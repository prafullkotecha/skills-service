{
  "name": "@skilltree/skills-client-display",
  "main": "./dist/userSkills.common.js",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "description": "SkillTree Client Display UI",
  "author": "SkillTree Team",
  "scripts": {
    "serve": "vue-cli-service serve",
    "licenseDepCheck": "license-checker --production --onlyAllow='MIT;ISC;Apache-2.0;Custom: https://travis-ci.org/component/emitter.png' --summary",
    "licenseHeaderCheck": "license-check-and-add check -f '../license-add/license-add-config.json' -r 20",
    "licenseHeaderAdd": "license-check-and-add add -f ../license-add/license-add-config.json -r 21",
    "licenseCheck": "npm run licenseHeaderCheck && npm run licenseDepCheck",
    "build": "npm run licenseCheck && npm run test:unit && vue-cli-service build",
    "build-lib": "NODE_ENV='production' && rm -rfv dist && vue-cli-service build --target lib --name userSkills './src/index.js'",
    "deploy": "npm run build && mkdir -p ../service/src/main/resources/public/static/ && mkdir -p ../service/target/classes/public/static && rm -rf ../service/src/main/resources/public/static/clientPortal && cp -rT dist ../service/src/main/resources/public/static/clientPortal && cp -rT dist ../service/target/classes/public/static/clientPortal ",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "animate.css": "4.1.1",
    "apexcharts": "3.26.1",
    "axios": "0.28.0",
    "axios-auth-refresh": "3.1.0",
    "bootstrap": "5.0.0",
    "bootstrap-vue": "2.21.2",
    "dayjs": "1.9.6",
    "dompurify": "2.5.4",
    "lodash": "4.17.21",
    "marked": "4.0.10",
    "material-icons": "0.3.1",
    "node-emoji": "1.10.0",
    "number-format.js": "2.0.9",
    "postmate": "1.5.2",
    "q": "1.5.1",
    "tinycolor2": "1.4.1",
    "url-search-params-polyfill": "8.1.0",
    "vis": "4.21.0",
    "vue": "3.0.0",
    "vue-apexcharts": "1.6.1",
    "vue-js-toggle-button": "1.3.3",
    "vue-radial-progress": "0.3.2",
    "vue-router": "3.4.3",
    "vue-simple-progress": "1.1.1",
    "vue-simple-spinner": "1.2.10",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@vue/eslint-config-airbnb": "5.1.0",
    "@vue/test-utils": "1.1.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.2.2",
    "license-check-and-add": "3.0.4",
    "license-checker": "25.0.1",
    "node-sass": "9.0.0",
    "sass-loader": "10.0.2",
    "vue-template-compiler": "2.7.16"
  },
  "peerDependencies": {
    "font-awesome": "4.7.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ]
}
